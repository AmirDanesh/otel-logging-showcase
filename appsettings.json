{
  "OpenTelemetry": {
    "ServiceName": "MyCoolApp.Api",
    "OtlpEndpoint": "http://localhost:4317",
    "UseConsoleExporter": false,
    "IncludeAspNetInstrumentation": true,
    "IncludeHttpClientInstrumentation": true,
    "IncludeRuntimeInstrumentation": false,
    "EnableRequestLoggingMiddleware": true,
    "CustomMetrics": [ "A", "B", "C" ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
